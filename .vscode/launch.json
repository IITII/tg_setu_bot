{
  "version": "0.2.0",
  "configurations": [
    {
      "command": "npm start",
      "name": "DebugTerminal",
      "request": "launch",
      "type": "node-terminal"
    },
    {
      "name": "start",
      "program": "${workspaceFolder}/App.js",
      "request": "launch",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "node"
    },
    {
      "name": "dev",
      "program": "${workspaceFolder}/App.js",
      "request": "launch",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "env": {
        "TG_DEBUG": "true"
      },
      "type": "node"
    }
  ]
}